{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "decode-jwt",
  "title": "Decode JWT",
  "description": "Decode a JSON Web Token (JWT) from the clipboard",
  "icon": "command-icon.png",
  "author": "klanchman",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "decode",
      "title": "Decode JWT",
      "description": "Display the decoded content of a JWT",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.32.1",
    "js-base64": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
